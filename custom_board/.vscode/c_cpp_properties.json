{
    "configurations": [
        {
            "name": "STM32 F0",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "USE_HAL_DRIVER",
                "STM32F091xC"
            ],
            
            "windowsSdkVersion": "10.0.22000.0",
            "configurationProvider": "ms-vscode.makefile-tools",
            "intelliSenseMode": "gcc-arm",
            "compilerPath": "${env:ARMGCC_DIR}/arm-none-eabi-gcc.exe",
            "compilerArgs" : [
                "-mcpu=cortex-m0",
                "-mthumb"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
        }
    ],
    "version": 4
}